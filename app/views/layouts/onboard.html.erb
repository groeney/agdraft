<!DOCTYPE html>
<html>
  <head>
    <title>AgDraft</title>
    <%= favicon_link_tag %>
    <%= stylesheet_link_tag    "onboard", media: "all", "data-turbolinks-track" => true %>
    <%= javascript_include_tag "onboard", "data-turbolinks-track" => true %>
    <%= csrf_meta_tags %>
    <%= render "includes/analytics" %>
  </head>
  <body class="<%= controller_name %> <%= action_name %>">
    <%= render "includes/onboard_menu" %>
    <%= render "includes/flash_notices" %>
    <div class="ui container" style="padding-bottom: 5%;">
      <%= yield %>
    </div>
  </body>
</html>
